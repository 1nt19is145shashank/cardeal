<html>
    <head>
          <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
          <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/popper.js@1.12.9/dist/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
    </head>
    <body>
      <form action="/detailstore" method="POST">
  <div class="form-row">
    <div class="form-group col-md-6">
      <label for="inputEmail4">Car ID</label>
      <input type="text" name="cid"class="form-control" id="inputEmail4" placeholder="KA04 5413">
    </div>
    <div class="form-group col-md-6">
      <label for="inputEmail4">Buyer Name</label>
      <input type="text" name="bname"class="form-control" id="inputEmail4" placeholder="ABC ABC">
    </div>
    </div>
    <div class="form-group col-md-6">
      <label for="inputEmail4">Phone Number</label>
      <input type="text" name="phno"class="form-control" id="inputEmail4" placeholder="123456789">
    </div>
    <div class="form-group col-md-6">
      <label for="inputPassword4">Email</label>
      <input type="email" name="bmail"class="form-control" id="inputPassword4" placeholder="Buyer Email">
    </div>
  <div class="form-group">
    <label for="inputAddress">Address</label>
    <input type="text" name="addr" class="form-control" id="inputAddress" placeholder="Address...">
  </div>
  <div class="form-group">
    <label for="inputAddress2">Address 2</label>
    <input type="text" name="addr2"class="form-control" id="inputAddress2" placeholder="Apartment, studio, or floor">
  </div>
  <div class="form-row">
    <div class="form-group col-md-6">
      <label for="inputCity">City</label>
      <input type="text" name="city"class="form-control" id="inputCity">
    </div>
    <div class="form-group col-md-4">
      <label for="inputState">State</label>
      <select id="inputState" class="form-control">
        <option selected>Choose..</option>
        <option>karnataka</option>
        <option>Andra Pradesh</option>
        <option>Kerala</option>
        <option>Tamil Nadu</option>
        <option>Goa</option>
        <option>Maharastra</option>
        <option>Telangana</option>
      </select>
    </div>
    <div class="form-group col-md-2">
      <label for="inputZip">Zip</label>
      <input type="text" name="pin" class="form-control" id="inputZip">
    </div>
  </div>
  <button type="submit" class="btn btn-primary">submit</button>
</form>
    </body>
</html>